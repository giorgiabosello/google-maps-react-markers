{
	"name": "google-maps-react-markers",
	"version": "1.1.1",
	"description": "Google Maps library with markers as react components",
	"author": "Giorgia Bosello",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/giorgiabosello/google-maps-react-markers.git"
	},
	"homepage": "https://giorgiabosello.github.io/google-maps-react-markers/",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.js",
	"keywords": [
		"react",
		"google-maps",
		"markers",
		"map",
		"google",
		"maps",
		"google-maps-react",
		"google-maps-react-markers",
		"react-component"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "microbundle-crl --no-compress --format modern,cjs",
		"start": "microbundle-crl watch --no-compress --format modern,cjs",
		"prepare": "run-s build",
		"test": "run-s test:unit test:lint test:build",
		"test:build": "run-s build",
		"test:lint": "eslint .",
		"test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
		"test:watch": "react-scripts test --env=jsdom",
		"predeploy": "cd docs && yarn install && yarn run build",
		"deploy": "gh-pages -d docs/build"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"babel-eslint": "^10.0.3",
		"cross-env": "^7.0.2",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-config-standard-react": "^13.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx": "^0.1.0",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-typescript": "^0.14.0",
		"gh-pages": "^5.0.0",
		"microbundle-crl": "^0.13.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "^5.0.1",
		"typescript": "^4.9.5"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@babel/plugin-syntax-flow": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.20.13",
		"prop-types": "^15.8.1"
	}
}
