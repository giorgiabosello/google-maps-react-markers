{
	"name": "google-maps-react-markers",
	"version": "1.2.4",
	"description": "Google Maps library with markers as react components",
	"author": "Giorgia Bosello",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/giorgiabosello/google-maps-react-markers.git"
	},
	"homepage": "https://giorgiabosello.github.io/google-maps-react-markers/",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.js",
	"keywords": [
		"react",
		"google-maps",
		"markers",
		"map",
		"google",
		"maps",
		"google-maps-react",
		"google-maps-react-markers",
		"react-component"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "microbundle-crl --no-compress --format modern,cjs",
		"dev": "microbundle-crl watch --no-compress --format modern,cjs",
		"prepare": "run-s build",
		"lint:fix": "eslint --fix src",
		"predeploy": "cd docs && yarn install && yarn build",
		"deploy": "gh-pages -d docs/build",
		"publish": "yarn build && npm publish"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/eslint-parser": "^7.21.3",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-react": "^7.18.6",
		"cross-env": "^7.0.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-config-standard-react": "^13.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx": "^0.1.0",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-typescript": "^0.14.0",
		"gh-pages": "^5.0.0",
		"microbundle-crl": "^0.13.11",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "^5.0.1",
		"typescript": "^5.0.4"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@babel/plugin-syntax-flow": "^7.21.4",
		"@babel/plugin-transform-react-jsx": "^7.21.5",
		"prop-types": "^15.8.1"
	}
}
